        },
        {
            "text": "IDEA",
            // "backgroundColor": "#B056AD" // poplux
            "backgroundColor": "#cc5595" // chinolor
        },
        "XXX"
    ],

    "diffEditor.ignoreTrimWhitespace": false,
    "git.autofetch": true,
    "githubPullRequests.queries": [

        {
            "label": "Waiting For My Review",
            "query": "is:open review-requested:${user}"
        },
        {
            "label": "Needs Review",
            "query": "is:open review:required -author:${user}"
        },
        {
            "label": "Created By Me",
            "query": "is:open author:${user}"
        },
        {
            "label": "Approved",
            "query": "is:open review:approved"
        }
    ],
    "editor.tabSize": 2,
    "scm.inputFontFamily": "editor",
    "materialTheme.accent": "Graphite",
    "files.insertFinalNewline": true,
    "colorize.languages": [
        "css",
        "sass",
        "scss",
        "less",
        "postcss",
        "sss",
        "stylus",
        "xml",
        "svg",
        "markdown"
    ],
    "gitlens.codeLens.enabled": false,
    "gitlens.views.repositories.location": "scm",
    "gitlens.views.fileHistory.location": "explorer",
    "gitlens.views.lineHistory.location": "explorer",
    "gitlens.views.compare.location": "gitlens",
    "gitlens.views.search.location": "gitlens",
    "gitlens.menus": {
        "editorGroup": false,
        "editorTab": false,
        "explorer": false,
        "scmGroup": {
            "compare": true,
            "openClose": true,
            "stash": true,
            "stashInline": true
        },
        "scmItem": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true,
            "stash": true
        }
    },
    "editor.renderWhitespace": "trailing",
    "gruvboxMaterial.italicKeywords": true,
    "gruvboxMaterial.lightContrast": "hard",
    "gruvboxMaterial.lightWorkbench": "high-contrast",
    "gruvboxMaterial.darkWorkbench": "high-contrast",
    "editor.fontSize": 14,

    "vim.normalModeKeyBindingsNonRecursive": [
        {
          "before": ["<space>"],
          "commands": ["vspacecode.space"]
        }
      ],
      "vim.visualModeKeyBindingsNonRecursive": [
        {
          "before": ["<space>"],
          "commands": ["vspacecode.space"]
        }
    ],
    "vim.statusBarColorControl": false,
    "vim.statusBarColors.normal": [
        "#8FBCBB",
        "#434C5E"
    ],
    "vim.statusBarColors.insert": "#BF616A",
    "vim.statusBarColors.visual": "#B48EAD",
    "vim.statusBarColors.visualline": "#B48EAD",
    "vim.statusBarColors.visualblock": "#A3BE8C",
    "vim.statusBarColors.replace": "#D08770",
    "vim.statusBarColors.commandlineinprogress": "#007ACC",
    "vim.statusBarColors.searchinprogressmode": "#007ACC",
    "vim.statusBarColors.easymotionmode": "#007ACC",
    "vim.statusBarColors.easymotioninputmode": "#007ACC",
    "vim.statusBarColors.surroundinputmode": "#007ACC",
    "editor.minimap.renderCharacters": false
 }
