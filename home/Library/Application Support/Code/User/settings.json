    "editor.fontWeight": 300,
    "editor.fontLigatures": true,
    "editor.minimap.enabled": true,
    "[markdown]": {
        "editor.wordWrap": "bounded",
        "editor.wrappingIndent": "indent",
    },
    "editor.colorDecorators": false,
    "files.defaultLanguage": "markdown",
    "workbench.colorCustomizations": {
        "[Pop N' Lock Theme by Luxcium ✨]": {
            "terminal.background": "#1D1028",
            "terminal.foreground": "#B1A68F",
            "terminal.ansiBlack": "#2C203C",
            "terminal.ansiBlue": "#528FC6",
            "terminal.ansiCyan": "#3C9C85",
            "terminal.ansiGreen": "#65A926",
            "terminal.ansiMagenta": "#B056AD",
            "terminal.ansiRed": "#C42251",
            "terminal.ansiWhite": "#C4BDB5",
            "terminal.ansiYellow": "#E2B651",
            "terminal.ansiBrightBlack": "#5D5D6F",
            "terminal.ansiBrightBlue": "#87CEFA",
            "terminal.ansiBrightCyan": "#8EB7AE",
            "terminal.ansiBrightGreen": "#A2BC4D",
            "terminal.ansiBrightMagenta": "#DA70D6",
            "terminal.ansiBrightRed": "#C4586A",
            "terminal.ansiBrightWhite": "#EAE7E4",
            "terminal.ansiBrightYellow": "#F3CD03"
        },
        "[Chinolor Light]": {
            "terminal.ansiBrightBlue": "#1781b5",
            "terminal.ansiBrightRed": "#bd3c45"
        },
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "comment, comment.line.double-slash",
                "settings": {
                    // "fontStyle": "italic"
                }
            },
            { // WIP not sure specificity is correct here
                "scope": "string.quoted, string.template",
                "settings": {
                    // "fontStyle": "italic"
                }
            },
            {
                "scope": "punctuation.definition.string, punctuation.definition.string.template, punctuation.definition.template-expression",
                "settings": {
                    "fontStyle": ""
                }
            }
        ],
        "[Chinolor Light]": {
            "textMateRules": [
                
            ]
        },
        "[Spacemacs - light]": {
            // "comments": "#969896", // too dark
            // "comments": "#c5c8c6", // too light
            "comments": "#adb0ae", // just right
            "textMateRules": [
                { // was making all JSON hash keys italic
                    "scope": "support.type",
                    "settings": {
                        "fontStyle": ""
                    }
                }
            ]
        },

        "[Pop N' Lock Theme by Luxcium ✨]": {
            "textMateRules": [
                // Make JS comments look like others
                {
                    "scope": "comment.line.double-slash, source.css comment.block",
                    "settings": {
                        "foreground": "#666677"
                    }
                },
                {
                    "scope": "comment.line.double-slash punctuation.definition.comment, comment.block punctuation.definition.comment.begin, comment.block punctuation.definition.comment.end",
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#506899"
                    }
                },

                // Style symbols
                {
                    "scope": "constant.language.symbol",
                    "settings": {
                        "foreground": "#55BBEE"
                    }
                },
                {
                    "scope": "constant.language.symbol punctuation.definition.constant",
                    "settings": {
                        "foreground": "#55BBEE88",
                        "fontStyle": "bold"
                    }
                },


                // Style  Markdown Headings
                {
                    "scope": "punctuation.definition.heading.markdown",
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#506899"
                    }
                },
                {
                    "scope": "entity.name.section.markdown",
                    "settings": {
                        "fontStyle": "italic underline",
                        "foreground": "#B056AD"
                    }
                },
                {
                    "scope": "heading.1.markdown entity.name.section.markdown",
                    "settings": {
                        "fontStyle": "italic bold underline",
                    }
                },

                // Style true/false/nil
                {
                    "scope": "constant.language.boolean",
                    "settings": {
                        "fontStyle": "bold italic"
                    }
                },
                {
                    "scope": "constant.language.nil",
                    "settings": {
                        "foreground": "#EE6677CC",
                        "fontStyle": "bold italic"
                    }
                }
            ]
        }
    },
    "atomKeymap.promptV3Features": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": true,
    "window.zoomLevel": 0,
    "editor.rulers": [

        {
            "column": 80,
            "color": "#4AE3"
        },
        {
            "column": 120,
            "color": "#FA04"
        },
        {
            "column": 121,
            "color": "#FA08"
        },
    ],
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.drawBoldTextInBrightColors": false,
    // "terminal.integrated.fontWeight": "300",
    // "better-comments.multilineComments": true,
    // "gitlens.codeLens.enabled": false,
    // "gitlens.currentLine.enabled": false
    "ruby.useBundler": true, //run non-lint commands with bundle exec
    "ruby.useLanguageServer": false, // use the internal language server (see below)
    "editor.acceptSuggestionOnEnter": "off",
    "workbench.colorTheme": "Chinolor Light",
    "workbench.iconTheme": "material-icon-theme",

    "todohighlight.isEnable": true,

    // these are set up for Pop Lux
    "todohighlight.defaultStyle": {
        // "color": "#2C203C",
        // "backgroundColor": "#5D5D6F",

        // chinolor dark
        // "color": "#2b312c",
