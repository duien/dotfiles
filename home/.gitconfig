[user]
	name = Emily Price
	email = price.emily@gmail.com
[color]
  ui = auto
[pull]
  default = matching
[push]
  default = matching
[core]
	excludesfile = /Users/eprice/.gitignore
  ; excludesfile = /home/eprice/.gitignore
  ; excludesfile = /home/duien/.gitignore
  editor = vim
[alias]
  vis = "log --graph --pretty=format:\"%C(yellow bold)%h%Creset by %C(red)%an%Creset (%ar)%C(blue bold)%d%Creset%n%s%n%b\" --all"
  history = "log --oneline --decorate --all --graph"
  hist = "log --oneline --decorate --all --graph"
	co = checkout
  pr = "pull --rebase"
[format]
  pretty = format:%C(yellow)%h %C(red)%aN%Creset %C(blue)%d%Creset %s
[difftool "Kaleidoscope"]
  cmd = ksdiff-wrapper git \"$LOCAL\" \"$REMOTE\"
[difftool]
  prompt = false
[diff]
  tool = Kaleidoscope
[github]
  user = duien
[branch]
  autosetupmerge = always
[mailmap]
  file = /Users/eprice/.gitmailmap
[pivotal]
